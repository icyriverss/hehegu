<table class="data-load" border="0" cellspacing="0" cellpadding="0" data-min="1200">
    <tr>
        <th width="80">订单类别</th>
        <th width="100">制单门店</th>
        
        <th width="150">单据编号</th>
        <th class="txtleft">发货仓库</th>
        <th width="100">处理状态</th>
        <th width="80">制单人</th>
        
        <th width="80">制单日期</th>
        <th width="60">操作</th>
    </tr>
    {foreach $list as $k=>$v}
        <tr>
            <td>{$v['order_name']}</td>
            <td>{getStoreName($v['store_id'])}</td>
            <td><a data-id="{$v['order_id']}">{$v['order_id']}</a></td>
            <td class="txtleft">{$v['bagname']}</td>
            <td>{$v['statusTxt']}</td>
            <td>{$v['user_name']}</td>
            <td>{$v['adddate']}</td>
            <td><a class="submit_order" data-id="{$v['order_id']}">审核</a></td>
        </tr>
    {/foreach}
    <tr class="showpage"><th colspan="8"></th></tr>
</table>
