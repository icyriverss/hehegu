        <td>{$info['rijun']}</td>
        <td>{$info['keliuliang']}</td>
        <td>{$info['kedanjia']}</td>
        <td>{$info['zujin']}</td>
        <td>{$info['chengben']}</td>
        <td>{$info['feiyong']}</td>
        <td>{$info['touzi']}</td>
