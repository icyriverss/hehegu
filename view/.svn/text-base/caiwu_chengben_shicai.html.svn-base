<table class="data-load"  border="0" cellspacing="0" cellpadding="0" ><thead>
    <tr>
        <th width="90" rowspan="2">食材编码</th>
        <th rowspan="2" class="txtleft">名称</th>
        <th width="90" rowspan="2">规格</th>
        <th width="60" rowspan="2">单位</th>
        <th width="60" rowspan="2">期初</th>
        <th width="60" rowspan="2">进货</th>
        <th width="60" rowspan="2">调出</th>
        <th width="60" rowspan="2">调入</th>
        <th width="60" rowspan="2">报损</th>
        <th width="60" rowspan="2">盘点</th>
        <th width="70" rowspan="2">实际用量</th>
        <th width="70" rowspan="2">理论用量</th>
        <th width="60" rowspan="2">单价</th>
        <th colspan="3">含报损分析</th>
        <th colspan="3">不含报损分析</th>
    </tr>
    <tr>
        <th width="55">量差异</th>
        <th width="55">偏差额</th>
        <th width="55">偏差率</th>
        <th width="55">量差异</th>
        <th width="55">偏差额</th>
        <th width="55">偏差率</th>
    </tr>
</thead>
<tbody>
    {foreach $list as $id=>$info}
    <tr >
        <th>{$info['bianhao']}</th>
        <td class="txtleft">{$info['name']}</td>
        <td>{$info['guige']}</td>
        <td>{$info['kucun_danwei']}</td>
        <td>{$info['qichu']}</td>
        <td>{$info['jinhuo']}</td>
        <td>{$info['diaochu']}</td>
        <td>{$info['diaoru']}</td>
        <td>{$info['baosun']}</td>
        <td>{$info['pandian']}</td>
        <td {if $info['liangchayi']>0}style="color:green;"{else if $info['liangchayi']<0}style="color:red;"{/if}>{$info['order_use']}</td>
        <td {if $info['liangchayi']>0}style="color:green;"{else if $info['liangchayi']<0}style="color:red;"{/if}>{$info['real_use']}</td>
        <td>{$info['cost']}</td>

        <td >{$info['liangchayi']}</td>
        <td >{$info['pianchae']}</td>
        <td>{$info['pianchalv']}%</td>

        <td>{$info['liangchayi_nbs']}</td>
        <td>{$info['pianchae_nbs']}</td>
        <td>{$info['pianchalv_nbs']}%</td>
    </tr>
    {/foreach}
</tbody></table>

