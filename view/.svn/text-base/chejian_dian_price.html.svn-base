
<table class="data-load"  border="0" cellspacing="0" cellpadding="0" >
<tbody >
    <tr>
        <th width="100">表编号：</th>
        <th class="txtleft">表名</th>
        <th width="100">倍率</th>
        <th width="100">单价</th>
        <th width="150">生效日期</th>
    </tr>
    {foreach $list as $k=>$v}
    <tr data-id="{$v['biao_id']}" class="biao_list">
        <th>{$v['biao_id']}</th>
        <td class="txtleft">{$v['biao_name']}</td>
        <td><input type="text" name="beilv" class="input4" value="{$v['beilv']}"></td>
        <td><input type="text" name="price" class="input4" value="{$v['price']}"></td>
        <td><input type="text" name="startdate" class="input4" value="{$v['startdate']}" onfocus="WdatePicker()" style="width:100px;" readonly></td>
    </tr>
    {/foreach}
    <tr>
        <th colspan="5" height="50">
            <input type="submit" value="提交" class="input-submit">
            </th>
        </tr>
</tbody></table>

